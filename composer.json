{
    "name": "timkippdev/affiliatizer",
    "description": "Add affiliate info to any configured urls",
    "autoload" : {
      "psr-4" : {
        "TimKippDev\\Affiliatizer\\" : "src/"
      }
    },
    "autoload-dev" : {
      "psr-4" : {
        "Tests\\" : "tests/"
      }
    },
    "require": {
      "php": "^7.1.3"
    },
    "require-dev": {
      "phpunit/phpunit": "^7.0"
    },
    "config": {
      "discard-changes": true,
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
  